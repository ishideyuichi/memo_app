<div class="f-container">
  <h1 class="f-item-h1">メモアプリ</h1>
  <form action="/new" method="get" class="f-item-submit">
    <input type="submit" value="メモ追加" class="btn btn-outline-primary ml-2">
  </form>
</div>
<ul>
  <% if @memo_array %>
  <% @memo_array.each do |memo| %>
  <li class="mt-3">
    <a href="/<%= memo.id %>"><%= memo.title %></a>
  </li>
  <% end %>
  <% end %>
</ul>
